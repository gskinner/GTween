package  {		import com.gskinner.motion.GTween;		import flash.display.MovieClip;	import flash.events.MouseEvent;		public class GTweenSpeedDemo extends MovieClip {			// Constants:			// Public Properties:			// Protected Properties:			// Initialization:		public function GTweenSpeedDemo() {			for (var i:uint=0; i<5000; i++) {				var thing:Thing = new Thing();				thing.x = thing.y = 300;				addChild(thing);				var tween:GTween = new GTween(thing,0.5,{x:Math.random()*600,y:Math.random()*600},{delay:Math.random(),onComplete:onComplete});			}		}			// Public getter / setters:			// Public Methods:			// Protected Methods:		protected function onComplete(tween:GTween):void {			var targ:Thing = tween.target as Thing;			targ.x = targ.y = 300;			var tween:GTween = new GTween(targ,0.5,{x:Math.random()*600,y:Math.random()*600},{delay:Math.random(),onComplete:onComplete});		}				protected function handleMouseMove(evt:MouseEvent):void {			GTween.timeScaleAll = stage.mouseX/600;		}	}	}