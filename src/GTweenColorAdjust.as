/*GTweenColorAdjust for GTween by Grant Skinner, gskinner.com/blog/This example demonstrates using the color adjust plugin.*/package  {		import com.gskinner.motion.GTween;	import com.gskinner.motion.plugins.ColorAdjustPlugin;		import flash.display.MovieClip;	import flash.display.Sprite;		public class GTweenColorAdjust extends MovieClip {			// Constants:			// Public Properties:			// Protected Properties:		public var image:Sprite;			// Initialization:		public function GTweenColorAdjust() {			ColorAdjustPlugin.install();			new GTween(image, 3, {saturation:-100, contrast:70}, {repeatCount:0, reflect:true});		}			// Public getter / setters:			// Public Methods:			// Protected Methods:			}	}