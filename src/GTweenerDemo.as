package  {		import com.gskinner.motion.GTweener;	import flash.display.MovieClip;	import flash.events.MouseEvent;		public class GTweenerDemo extends MovieClip {			// Constants:			// Public Properties:			// Protected Properties:			// Initialization:		public function GTweenerDemo() {			GTweener.to(ball, 2, {x:200, y:350}, {repeatCount:0, reflect:true});			stage.addEventListener(MouseEvent.CLICK, handleClick);		}			// Public getter / setters:			// Public Methods:			// Protected Methods:		protected function handleClick(evt:MouseEvent):void {			GTweener.to(ball, 0.4, {x: mouseX});		}	}	}